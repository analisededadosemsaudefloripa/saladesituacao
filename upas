  options = list(
    "dom" = 'T<"clear">lBfrtip',
    buttons = list('copy', 'csv', 'pdf', 'print')))
  
  
  #informações odontologia sul por hora do dia
  output$num_atd_odontologia_sul_hora_info <- renderText({
    
